<body class = "doctor_signin_body" >
<div class = "doctor_signin_grid_topleft">
</div>

<div class = "doctor_signin_form">
    <h1 class = "doctor_login_template" > Login To Your Account! </h1>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="field">
        <br />
        <%= f.email_field :email, autofocus: true, placeholder: "Email", autocomplete: "email", class: "doctor_signin_emailfield" %>
        <%= resource.errors.full_messages_for(:email).first %>
      </div>

      <div class="field">
        <br />
        <%= f.password_field :password, placeholder: "Password", autocomplete: "current-password", class: "doctor_signin_passfield" %>
        <%= resource.errors.full_messages_for(:email).first %>
      </div>

      <% if devise_mapping.rememberable? %>
        <div class="doctor_signin_remember">
          <%= f.check_box :remember_me %>
          <%= f.label :remember_me %>
        </div>
      <% end %>

      <div class="doctor_signin_login_button">
        <%= f.submit "Log in" %>
      </div>
    <% end %>
    <div class = "doctor_signin_links" >
      <%= render "doctors/shared/links" %>
    </div>
</div>

<div class = "doctor_signin_image" >
</div>

<div class = "doctor_signin_border_right">
</div>

<div class = "doctor_signin_grid_bottomright">  
</div>